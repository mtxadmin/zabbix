
<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-06-17T16:00:00Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Windows Scheduled Tasks</template>
            <name>Template Windows Scheduled Tasks</name>
            <description>Discover and monitor Windows scheduled tasks. Need to add powershell script to agent on hosts.</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Scheduled tasks</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Discovery Task Scheduler</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>TaskSchedulerMonitoring[DiscoverCustomTasks]</key>
                    <delay>60</delay>
                    <filter>
                        <evaltype>FORMULA</evaltype>
                        <formula>A and B and C and D</formula>
                        <conditions>
                            <condition>
                                <macro>{#APPTASKS}</macro>
                                <value>User_Feed_Synchronization-</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>C</formulaid>
                            </condition>
                            <condition>
                                <macro>{#APPTASKS}</macro>
                                <value>{$SCHEDULED.TASK.MATCHES}</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#APPTASKS}</macro>
                                <value>{$SCHEDULED.TASK.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>B</formulaid>
                            </condition>
                            <condition>
                                <macro>{#APPTASKS}</macro>
                                <value>Adobe Acrobat Update Task</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>D</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>0</lifetime>
                    <description>Discover all Windows scheduled tasks. This discovery needs powershell script in the agent host.</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>Task: {#APPTASKS}: Last Result</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}]</key>
                            <delay>120</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>Last Result of Task, string hex value&#13;
 in most cases 0 = succesful</description>
                            <applications>
                                <application>
                                    <name>Scheduled tasks</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Scheduled Task</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>MATCHES_REGEX</type>
                                    <params>^0x</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Task: {#APPTASKS}: Last Run Time</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>TaskSchedulerMonitoring[TaskLastRunTime,{#APPTASKS}]</key>
                            <delay>120</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <description>Last Run Time of Task as an unixtimestamp</description>
                            <applications>
                                <application>
                                    <name>Scheduled tasks</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Task: {#APPTASKS}: Next Run Time</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>TaskSchedulerMonitoring[TaskNextRunTime,{#APPTASKS}]</key>
                            <delay>120</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <description>Next Run Time of Task</description>
                            <applications>
                                <application>
                                    <name>Scheduled tasks</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Task: {#APPTASKS}: Task State</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>TaskSchedulerMonitoring[TaskState,{#APPTASKS}]</key>
                            <delay>120</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>TaskState, string value</description>
                            <applications>
                                <application>
                                    <name>Scheduled tasks</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Scheduled Task</name>
                            </valuemap>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskState,{#APPTASKS}].last()}=&quot;Disabled&quot; or {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}=&quot;0x41302&quot;</expression>
                            <name>Scheduled task {#APPTASKS} disabled</name>
                            <status>DISABLED</status>
                            <priority>AVERAGE</priority>
                            <description>Trigger for detecting event when a task has been disabled.&#13;
Such triggers created with disabled state</description>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x41300&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x41301&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x41302&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x41303&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x41306&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0x800710E0&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0xC0020001&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskLastResult,{#APPTASKS}].last()}&lt;&gt;&quot;0xFFFFFFFF&quot; and {Template Windows Scheduled Tasks:TaskSchedulerMonitoring[TaskState,{#APPTASKS}].last()}&lt;&gt;&quot;Disabled&quot;</expression>
                            <name>Scheduled task {#APPTASKS} ended with error</name>
                            <priority>AVERAGE</priority>
                            <description>Excluded status codes:&#13;
0 - Success&#13;
0x41300 - Ready&#13;
0x41301 - Running&#13;
0x41302 - Disabled&#13;
0x41303 - The task has not yet run&#13;
0x41306 - The task was terminated by user&#13;
0x800710E0 - The operator or administrator has refused the request&#13;
0xC0020001&#13;
0xFFFFFFFF - Unhandled error</description>
                        </trigger_prototype>
                    </trigger_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$SCHEDULED.TASK.MATCHES}</macro>
                    <value>^.*$</value>
                </macro>
                <macro>
                    <macro>{$SCHEDULED.TASK.NOT_MATCHES}</macro>
                    <value>^$</value>
                </macro>
            </macros>
        </template>
    </templates>
